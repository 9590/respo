{
  "name": "respo",
  "version": "0.1.0",
  "description": "FIXME: description",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "html": "cp assets/* target/",
    "watch": "shadow-cljs --build app --dev",
    "sv": "shadow-cljs --server",
    "cljs-test": "shadow-cljs --build test --once",
    "once": "shadow-cljs --build app --once",
    "release": "shadow-cljs --build release --release",
    "serve": "http-server target/ -s -c-1",
    "del": "rm -r target/*",
    "tiye": "rsync -r target/ repo.respo.site:repo/Respo/respo",
    "build": "yarn del && yarn html && yarn release && yarn serve"
  },
